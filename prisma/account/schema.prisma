generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_ACC_URL")
}

model user {
  id          Int       @id @default(autoincrement())
  username    String?
  email       String    @unique
  password    String
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @default(now())
}
